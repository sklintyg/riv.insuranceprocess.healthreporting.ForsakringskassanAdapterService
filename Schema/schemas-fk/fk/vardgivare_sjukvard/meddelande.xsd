<?xml version="1.0" encoding="ISO-8859-1"?><xs:schema    xmlns:xs="http://www.w3.org/2001/XMLSchema"    targetNamespace="http://schema.forsakringskassan.se/vardgivare_sjukvard"   xmlns="http://schema.forsakringskassan.se/vardgivare_sjukvard"    elementFormDefault="qualified"    version="1.0">   <xs:annotation>      <xs:documentation>2010-10-13</xs:documentation>   </xs:annotation>   <xs:simpleType name="ReferensLakarintyg">      <xs:restriction base="xs:string">         <xs:annotation>            <xs:documentation>Avser referens till läkarintyg</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:lakarintyg/VAS:referens</xs:documentation>         </xs:annotation>      </xs:restriction>   </xs:simpleType>   <xs:simpleType name="Signerades">      <xs:restriction base="xs:dateTime">         <xs:annotation>            <xs:documentation>Avser då läkarintyget, frågan eller svaret skrevs/signerades</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:lakarintyg/VAS:signerades</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:fraga/VAS:signerades</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:svar/VAS:signerades</xs:documentation>         </xs:annotation>      </xs:restriction>   </xs:simpleType>   <xs:complexType name="Falt">      <xs:sequence>         <xs:element name="namn">            <xs:simpleType>               <xs:annotation>                  <xs:documentation>Avser specifikt fält i angivet läkarintyg</xs:documentation>                  <xs:documentation>eDok/verksamhetsdata/VAS:lakarintyg/VAS:falt/VAS:namn</xs:documentation>               </xs:annotation>               <xs:restriction base="xs:string">                  <xs:enumeration value="1. Avstängning enligt smittskyddslagen på grund av smitta"/>                  <xs:enumeration value="2. Diagnos/diagnoser för sjukdom/symtom som orsakar nedsatt arbetsförmåga"/>                  <xs:enumeration value="3. Aktuellt sjukdomsförlopp"/>                  <xs:enumeration value="4. Funktionsnedsättning - observationer, undersökningsfynd och utredningsresultat"/>                  <xs:enumeration value="5. Aktivitetsbegränsning relaterat till diagnos (fält 2) och funktionsnedsättning (fält 4)"/>                  <xs:enumeration value="6a. Rekommendationer"/>                  <xs:enumeration value="6b. Planerad eller pågående behandling eller åtgärd"/>                  <xs:enumeration value="7. Är arbetslivsinriktad rehabilitering aktuell?"/>                  <xs:enumeration value="8a. Patientens arbetsförmåga bedöms i förhållande till"/>                  <xs:enumeration value="8b. Jag bedömer att patientens arbetsförmåga är"/>                  <xs:enumeration value="9. Jag bedömer att patientens arbetsförmåga är nedsatt längre tid än den som det försäkringsmedicinska beslutstödet anger, därför att:"/>                  <xs:enumeration value="10. Prognos - kommer patienten att få tillbaka sin arbetsförmåga i nuvarande arbete? (Gäller inte arbetslösa)"/>                  <xs:enumeration value="11. Kan resor till och från arbetet med annat färdsätt än det patienten normalt använder göra det möjligt för patienten att återgå till arbete?"/>                  <xs:enumeration value="12. Kontakt önskas med Försäkringskassan"/>                  <xs:enumeration value="13. Övriga upplysningar"/>                  <xs:enumeration value="14. Datum"/>                  <xs:enumeration value="15. Namnförtydligande, mottagningens adress, telefon"/>                  <xs:enumeration value="16. Läkarens namnteckning"/>                  <xs:enumeration value="17. Förskrivarkod och arbetsplatskod"/>               </xs:restriction>            </xs:simpleType>         </xs:element>         <xs:element name="kommentar" type="xs:string" minOccurs="0">            <xs:annotation>               <xs:documentation>Eventuell kommentar till angivet fält</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/VAS:lakarintyg/VAS:falt/VAS:kommentar</xs:documentation>            </xs:annotation>         </xs:element>      </xs:sequence>   </xs:complexType>   <xs:complexType name="Lakarintygsreferens"><!--      <xs:complexType name="Lakarintyg">-->      <xs:sequence>         <xs:element name="referens" type="ReferensLakarintyg"/>         <xs:element name="signerades" type="Signerades"/>         <xs:element name="falt" type="Falt" minOccurs="0" maxOccurs="unbounded"/>      </xs:sequence>   </xs:complexType>   <xs:complexType name="Amne">      <xs:sequence>         <xs:element name="beskrivning">            <xs:simpleType>               <xs:restriction base="xs:string">                  <xs:annotation>                     <xs:documentation>Avser meddelandets ämne</xs:documentation>                     <xs:documentation>eDok/verksamhetsdata/VAS:amne/VAS:namn</xs:documentation>                  </xs:annotation>                  <xs:enumeration value="Arbetstidsförläggning"/>                  <xs:enumeration value="Avstämningsmöte"/>                  <xs:enumeration value="Komplettering"/>                  <xs:enumeration value="Kontakt"/>                  <xs:enumeration value="Makulering"/>                  <xs:enumeration value="Påminnelse"/>                  <xs:enumeration value="Övrigt"/>               </xs:restriction>            </xs:simpleType>         </xs:element>         <xs:element name="fritext" minOccurs="0">            <xs:simpleType>               <xs:restriction base="xs:string">                  <xs:annotation>                     <xs:documentation>Avser eventuellt användarens egna explicit angivna ämne</xs:documentation>                     <xs:documentation>eDok/verksamhetsdata/VAS:amne/VAS:text</xs:documentation>                  </xs:annotation>               </xs:restriction>            </xs:simpleType>         </xs:element>      </xs:sequence>   </xs:complexType>   <xs:simpleType name="Text">      <xs:restriction base="xs:string">         <xs:annotation>            <xs:documentation>Avser meddelandets innehåll</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:fraga/VAS:text</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:svar/VAS:text</xs:documentation>         </xs:annotation>      </xs:restriction>   </xs:simpleType>   <xs:simpleType name="Besvaras">      <xs:restriction base="xs:date">         <xs:annotation>            <xs:documentation>Avser eventuellt datum då svar senast önskas</xs:documentation>            <xs:documentation>eDok/verksamhetsdata/VAS:besvaras</xs:documentation>         </xs:annotation>      </xs:restriction>   </xs:simpleType>   <xs:complexType name="Meddelande">      <xs:sequence>         <xs:element name="signerades" type="Signerades"/>         <xs:element name="text" type="Text"/>      </xs:sequence>   </xs:complexType></xs:schema>