<?xml version="1.0" encoding="ISO-8859-1"?><xs:schema    xmlns:xs="http://www.w3.org/2001/XMLSchema"    targetNamespace="http://schema.forsakringskassan.se/vardgivare_sjukvard"   xmlns="http://schema.forsakringskassan.se/vardgivare_sjukvard"    elementFormDefault="qualified"    version="1.0">   <xs:annotation>      <xs:documentation>2010-05-26</xs:documentation>   </xs:annotation>   <xs:complexType name="Lakarintyg">      <xs:sequence>         <xs:element name="smittskydd">            <xs:annotation>               <xs:documentation>1. Avstängning enligt smittskyddslagen på grund av smitta</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:avstangning</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string" fixed="Avstängning enligt smittskyddslagen på grund av smitta" minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:avstangningEnligtSmL</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="diagnos">            <xs:annotation>               <xs:documentation>2. Diagnos/diagnoser för sjukdom som orsakar nedsatt arbetsförmåga</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:diagnosForSjukdomEllerSymptom</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string"  minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:beskrivning</xs:documentation>                     </xs:annotation>                  </xs:element>                  <xs:element name="kod" type="xs:string" minOccurs="0">                     <xs:annotation>                        <xs:documentation>Diagnoskod enligt ICD-10</xs:documentation>                        <xs:documentation>SJP:diagnosKod</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="sjukdomshistoria">            <xs:annotation>               <xs:documentation>3. Aktuellt sjukdomsförlopp</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:sjukdomsforlopp</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string" minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:beskrivning</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="status">            <xs:annotation>               <xs:documentation>4. Funktionsnedsättning - observationer, undersökningsfynd och utredningsresultat</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:funktionsnedsattning</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string"  minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:objektivaUndersokningsFynd</xs:documentation>                     </xs:annotation>                  </xs:element>                  <xs:element name="basering">                     <xs:annotation>                        <xs:documentation>Konceptuellt borde minOccurs vara 1 här, men eftersom det inte är tvingande när det handlar om smittskydd, så blir vi tvungen att sätta det till 0</xs:documentation>                        <xs:documentation>SJP:uppgifterBaseradePa</xs:documentation>                     </xs:annotation>                     <xs:complexType>                        <xs:sequence minOccurs="0" maxOccurs="unbounded">                           <xs:element name="ursprung">                              <xs:complexType>                                 <xs:sequence>                                    <xs:element name="beskrivning">                                       <xs:simpleType>                                          <xs:restriction base="xs:string">                                             <xs:enumeration value="Undersökning">                                                <xs:annotation>                                                   <xs:documentation>SJP:undersokning/SJP:markeringsKryss</xs:documentation>                                                </xs:annotation>                                             </xs:enumeration>                                             <xs:enumeration value="Telefonkontakt">                                                <xs:annotation>                                                   <xs:documentation>SJP:telefonKontakt/SJP:markeringsKryss</xs:documentation>                                                </xs:annotation>                                             </xs:enumeration>                                             <xs:enumeration value="Journaluppgifter">                                                <xs:annotation>                                                   <xs:documentation>SJP:journalUppgifter/SJP:markeringsKryss</xs:documentation>                                                </xs:annotation>                                             </xs:enumeration>                                             <xs:enumeration value="Annat">                                                <xs:annotation>                                                   <xs:documentation>SJP:annat/SJP:markeringsKryss</xs:documentation>                                                </xs:annotation>                                             </xs:enumeration>                                          </xs:restriction>                                       </xs:simpleType>                                    </xs:element>                                    <xs:element name="datum" type="xs:date">                                       <xs:annotation>                                          <xs:documentation>[...]/SJP:datum</xs:documentation>                                       </xs:annotation>                                    </xs:element>                                 </xs:sequence>                              </xs:complexType>                           </xs:element>                        </xs:sequence>                     </xs:complexType>                     <xs:unique name="Unikt_ursprung">                        <xs:selector xpath="ursprung"></xs:selector>                        <xs:field xpath="beskrivning"></xs:field>                     </xs:unique>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>                  <xs:element name="begransning">            <xs:annotation>               <xs:documentation>5. Aktivitetsbegränsning relaterat till diagnos och funktionsnedsättning</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:begransandeAvFormaga</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string"  minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:beskrivning</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="rekommendationer">            <xs:annotation>               <xs:documentation>6a. Rekommendationer</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:rekommendationer</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="rekommendation" minOccurs="0" maxOccurs="unbounded">                     <xs:complexType>                        <xs:sequence>                           <xs:element name="beskrivning">                              <xs:simpleType>                                 <xs:restriction base="xs:string">                                    <xs:enumeration value="Arbetsförmedlingen">                                       <xs:annotation>                                          <xs:documentation>kontakt med Arbetsförmedlingen</xs:documentation>                                          <xs:documentation>SJP:kontaktMedArbetsformedlingen</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                    <xs:enumeration value="Företagshälsovården">                                       <xs:annotation>                                          <xs:documentation>kontakt med företagshälsovården</xs:documentation>                                          <xs:documentation>SJP:kontaktMedForetagshalsovarden</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                    <xs:enumeration value="Övrigt">                                       <xs:annotation>                                          <xs:documentation>övrigt (ange vad)</xs:documentation>                                          <xs:documentation>SJP:ovrigt/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                 </xs:restriction>                              </xs:simpleType>                           </xs:element>                           <xs:element name="kommentar" minOccurs="0">                              <xs:annotation>                                 <xs:documentation>övrigt (ange vad)</xs:documentation>                                 <xs:documentation>SJP:ovrigt/SJP:beskrivning</xs:documentation>                              </xs:annotation>                           </xs:element>                        </xs:sequence>                     </xs:complexType>                  </xs:element>               </xs:sequence>            </xs:complexType>            <xs:unique name="Unik_rekommendation">               <xs:selector xpath="rekommendation"></xs:selector>               <xs:field xpath="beskrivning"></xs:field>            </xs:unique>         </xs:element>         <xs:element name="planering">            <xs:annotation>               <xs:documentation>6b Planerad eller pågående behandling eller åtgärd</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:atgarder</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="behandling" minOccurs="0" maxOccurs="unbounded">                     <xs:complexType>                        <xs:sequence>                           <xs:element name="beskrivning">                              <xs:simpleType>                                 <xs:restriction base="xs:string">                                    <xs:enumeration value="Sjukvården">                                       <xs:annotation>                                          <xs:documentation>inom sjukvården (ange vad)</xs:documentation>                                          <xs:documentation>SJP:atgardSjukvard/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                    <xs:enumeration value="Annan">                                       <xs:annotation>                                          <xs:documentation>annan åtgärd (ange vad)</xs:documentation>                                          <xs:documentation>SJP:annanAtgard/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                 </xs:restriction>                              </xs:simpleType>                           </xs:element>                           <xs:element name="kommentar" minOccurs="0">                              <xs:annotation>                                 <xs:documentation>inom sjukvården (ange vad)</xs:documentation>                                 <xs:documentation>[...]/SJP:beskrivning</xs:documentation>                                 <xs:documentation>annan åtgärd (ange vad)</xs:documentation>                                 <xs:documentation>[...]/SJP:beskrivning</xs:documentation>                              </xs:annotation>                           </xs:element>                        </xs:sequence>                     </xs:complexType>                  </xs:element>                           </xs:sequence>            </xs:complexType>            <xs:unique name="Unik_behandling">               <xs:selector xpath="behandling"></xs:selector>               <xs:field xpath="beskrivning"></xs:field>            </xs:unique>         </xs:element>                  <xs:element name="rehabilitering">            <xs:annotation>               <xs:documentation>7. Är arbetslivsinriktad rehabilitering aktuell</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:arbetslivsinriktadRehab</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning"  minOccurs="0">                     <xs:simpleType>                        <xs:restriction base="xs:string">                           <xs:enumeration value="Arbetslivsinriktad rehabilitering är aktuell">                              <xs:annotation>                                 <xs:documentation>SJP:ja</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Arbetslivsinriktad rehabilitering är inte aktuell">                              <xs:annotation>                                 <xs:documentation>SJP:nej</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Behovet av arbetslivsinriktad rehabilitering går inte att bedöma">                              <xs:annotation>                                 <xs:documentation>SJP:garInteAttBedoma</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                        </xs:restriction>                     </xs:simpleType>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="sysselsattning">            <xs:annotation>               <xs:documentation>8a. Patientens arbetsförmåga bedöms i förhållande till</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:bedomdGradAvFunktionsnedsattning</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="arbetsuppgifter" type="xs:string" minOccurs="0">                     <xs:annotation>                        <xs:documentation>Avser informationen i ruta 8 som dock är en egenskap på patienten och inte på arbetsförmågan</xs:documentation>                        <xs:documentation>SJP:nuvarandeArbete/SJP:beskrivning</xs:documentation>                     </xs:annotation>                  </xs:element>                  <xs:element name="beskrivning" minOccurs="0" maxOccurs="unbounded">                     <xs:simpleType>                        <xs:restriction base="xs:string">                           <xs:enumeration value="Förvärvsarbetande">                              <xs:annotation>                                 <xs:documentation>nuvarande arbete</xs:documentation>                                 <xs:documentation>SJP:nuvarandeArbete/SJP:markeringsKryss</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Arbetslös">                              <xs:annotation>                                 <xs:documentation>SJP:arbetslos</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Föräldraledig">                              <xs:annotation>                                 <xs:documentation>SJP:foraldraledig</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                        </xs:restriction>                     </xs:simpleType>                  </xs:element>               </xs:sequence>            </xs:complexType>            <xs:unique name="Unik_beskrivning">               <xs:selector xpath="beskrivning"></xs:selector>               <xs:field xpath="."></xs:field>            </xs:unique>         </xs:element>                  <xs:element name="arbetsformaga">            <xs:annotation>               <xs:documentation>8b. Jag bedömer att patientens arbetsförmåga är</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:arbetsFormaga</xs:documentation>            </xs:annotation>                        <xs:complexType>               <xs:sequence>                  <xs:element name="nedsattningsgrad" maxOccurs="unbounded">                     <xs:complexType>                        <xs:sequence>                           <xs:element name="beskrivning">                              <xs:simpleType>                                 <xs:restriction base="xs:string">                                    <xs:enumeration value="Nedsatt med 1/4">                                       <xs:annotation>                                          <xs:documentation>SJP:fjardedelsNedsatt/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                    <xs:enumeration value="Nedsatt med hälften">                                       <xs:annotation>                                          <xs:documentation>SJP:halftenNedsatt/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                    <xs:enumeration value="Nedsatt med 3/4">                                       <xs:annotation>                                          <xs:documentation>SJP:treFjardedelsNedsatt/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                    <xs:enumeration value="Helt nedsatt">                                       <xs:annotation>                                          <xs:documentation>SJP:heltNedsatt/SJP:markeringsKryss</xs:documentation>                                       </xs:annotation>                                    </xs:enumeration>                                 </xs:restriction>                              </xs:simpleType>                           </xs:element>                           <xs:element name="franOchMed" type="xs:date">                              <xs:annotation>                                 <xs:documentation>[...]/SJP:fromDatum</xs:documentation>                              </xs:annotation>                           </xs:element>                           <xs:element name="langstTillOchMed" type="xs:date">                              <xs:annotation>                                 <xs:documentation>[...]/SJP:tomDatum</xs:documentation>                              </xs:annotation>                           </xs:element>                        </xs:sequence>                     </xs:complexType>                  </xs:element>               </xs:sequence>            </xs:complexType>            <xs:unique name="Unik_nedsättningsgrad">               <xs:selector xpath="nedsattningsgrad"></xs:selector>               <xs:field xpath="beskrivning"></xs:field>            </xs:unique>         </xs:element>                           <!-- svårt att förstå vad denna skall heta och var denna hör hemma och vad det handlar om ... i alla fall för en enkel utvecklare -->         <xs:element name="motivering">            <xs:annotation>               <xs:documentation>9. Patientens arbetsförmåga bedöms nedsatt längre tid än den som det försäkringsmedicinska beslutsstödet anger</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:arbetsformagaNedsattLangreTid</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string" minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:orsakNedsattningLangreAnBeslutstod</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>                           <xs:element name="prognos">            <xs:annotation>               <xs:documentation>10. Prognos - kommer patienten att få tillbaka sin arbetsförmåga i nuvarande arbete</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:prognos</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" minOccurs="0">                     <xs:simpleType>                        <xs:restriction base="xs:string">                           <xs:enumeration value="Patienten kommer att få tillbaka sin arbetsförmåga">                              <xs:annotation>                                 <xs:documentation>SJP:ja</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Patienten kommer delvis att få tillbaka sin arbetsförmåga">                              <xs:annotation>                                 <xs:documentation>SJP:jaDelvis</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Patienten kommer inte att få tillbaka sin arbetsförmåga">                              <xs:annotation>                                 <xs:documentation>SJP:nej</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Det går inte att bedöma om patienten får tillbaka sin arbetsförmåga">                              <xs:annotation>                                 <xs:documentation>SJP:kanEjBedoma</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                        </xs:restriction>                     </xs:simpleType>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="resor">            <xs:annotation>               <xs:documentation>11. Kan resor till och från arbetet med annat färdsätt än normalt göra det möjligt för patienten att återgå i arbete</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:mojliggorAnnatFardsattAtergangTillArbete</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" minOccurs="0">                     <xs:simpleType>                        <xs:restriction base="xs:string">                           <xs:enumeration value="Annat färdsätt kan möjliggöra återgång till arbetet">                              <xs:annotation>                                 <xs:documentation>SJP:ja</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                           <xs:enumeration value="Annat färdsätt kan inte möjliggöra återgång till arbetet">                              <xs:annotation>                                 <xs:documentation>SJP:nej</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                        </xs:restriction>                     </xs:simpleType>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="kontakt">            <xs:annotation>               <xs:documentation>12. Kontakt önskas med Försäkringskassan</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:onskarKontaktMedFK</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" minOccurs="0">                     <xs:simpleType>                        <xs:restriction base="xs:string">                           <xs:enumeration value="Kontakt önskas med Försäkringskassan">                              <xs:annotation>                                 <xs:documentation>SJP:ja</xs:documentation>                              </xs:annotation>                           </xs:enumeration>                        </xs:restriction>                     </xs:simpleType>                  </xs:element>                     </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="upplysningar">            <xs:annotation>               <xs:documentation>13. Övriga upplysningar och förtydliganden</xs:documentation>               <xs:documentation>eDok/verksamhetsdata/SJP:ovrigaUpplysningar</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="beskrivning" type="xs:string"  minOccurs="0">                     <xs:annotation>                        <xs:documentation>SJP:beskrivning</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>         <xs:element name="lakare">            <xs:annotation>               <xs:documentation>eDok/verksamhetsdata/SJP:lakarensUnderskrift</xs:documentation>            </xs:annotation>            <xs:complexType>               <xs:sequence>                  <xs:element name="signerades" type="xs:dateTime">                     <xs:annotation>                        <xs:documentation>14. Datum</xs:documentation>                        <xs:documentation>Skall avse när läkarintyget skrevs/signerades</xs:documentation>                        <xs:documentation>SJP:underskriftDatum</xs:documentation>                     </xs:annotation>                  </xs:element>                           <xs:element name="forskrivarkod" type="xs:string" minOccurs="0">                     <xs:annotation>                        <xs:documentation>17. Förskrivarkod och arbetsplatskod</xs:documentation>                        <xs:documentation>SJP:forskrivarkodOchArbetsplatskod</xs:documentation>                     </xs:annotation>                  </xs:element>                           <xs:element name="arbetsplatskod" type="xs:string">                     <xs:annotation>                        <xs:documentation>17. Förskrivarkod och arbetsplatskod</xs:documentation>                        <xs:documentation>SJP:forskrivarkodOchArbetsplatskod</xs:documentation>                     </xs:annotation>                  </xs:element>               </xs:sequence>            </xs:complexType>         </xs:element>      </xs:sequence>   </xs:complexType></xs:schema>